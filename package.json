{
  "name": "ww-music",
  "version": "0.0.6",
  "description": "A simple, TypeScript audio instrument and midi playback module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wwlib/ww-music.git"
  },
  "keywords": [
    "music",
    "midi",
    "audio",
    "instrument"
  ],
  "config": {
    "watch": "src",
    "app": "src/index.ts",
    "lib": "lib/*.*"
  },
  "scripts": {
    "build": "npm run clean; tsc",
    "clean": "cross-conf-env rimraf npm_package_config_lib",
    "test": "mocha"
  },
  "browserify": {
    "transform": []
  },
  "dependenciesDisabled": {
    "webmidi": "^3.0.20",
    "pie-ano": "^1.1.2"
  },
  "dependencies": {
    "audio-context-mock": "^0.1.3",
    "ensureDir": "^1.0.6",
    "find-root": "^1.1.0",
    "fs-extra": "^10.1.0",
    "midi-player-js": "^2.0.16",
    "soundfont-player": "^0.12.0"
  },
  "devDependencies": {
    "cross-conf-env": "latest",
    "mapstraction": "latest",
    "mocha": "^10.0.0",
    "parallelshell": "latest",
    "preprocessify": "latest",
    "rimraf": "latest",
    "tsify": "latest",
    "typescript": "^2.0.0"
  },
  "author": "Andrew Rapo <andrew@worthwhilegames.org> (http://worthwhilegames.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wwlib/ww-music/issues"
  },
  "homepage": "https://github.com/wwlib/ww-music#readme"
